{
	"name": "lh/edmunds",
	"description": "The fast PHP framework for building web applications.",
	"keywords": ["edmunds", "framework", "core", "package", "php"],
	"license": "http://lowiehuyghe.com/license",
	"authors": [
		{
			"name": "Lowie Huyghe",
			"email": "iam@lowiehuyghe.com"
		}
	],
	"type": "library",
	"require": {
		"php": ">=5.5.9",
		"laravel/lumen-framework": "5.2.*",
		"illuminate/session": "5.2.*",
		"illuminate/cookie": "5.2.*",
		"illuminate/filesystem": "5.2.*",
		"illuminate/mail": "5.2.*",
		"guzzlehttp/guzzle": "~5.3|~6.0",
		"league/flysystem": "~1.0",
		"vlucas/phpdotenv": "~2.2",
		"piwik/device-detector": "3.5.*",
		"geoip2/geoip2": "~2.0",
		"maknz/slack": "~1.7",
		"anahkiasen/former": "4.0.*",
		"fzaninotto/faker": "~1.4",
		"pusher/pusher-php-server": "~2.0",
		"predis/predis": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"psr-4": {
			"Edmunds\\": "src/Edmunds"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\": "app/",
			"EdmundsTest\\": "tests/Edmunds"
		},
		"classmap": [
			"tests/",
			"database/"
		]
	}
}

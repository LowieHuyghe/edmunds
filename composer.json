{
	"name": "lh/core",
	"description": "The core of any web-project by Lowie Huyghe",
	"keywords": ["core", "package", "Lowie", "Huyghe"],
	"license": "http://lowiehuyghe.com/license",
	"authors": [
		{
			"name": "Lowie Huyghe",
			"email": "iam@lowiehuyghe.com"
		}
	],
	"type": "library",
	"require": {
		"php": ">=5.5.9",
		"laravel/lumen-framework": "5.1.*",
		"vlucas/phpdotenv": "~1.0",
		"cbschuld/browser.php": "*@dev",
		"geoip2/geoip2": "~2.0",
		"pda/pheanstalk": "~3.0",
		"maknz/slack": "~1.7",
		"anahkiasen/former": "4.0.*@dev",
		"SobanVuex/php-newrelic": "~2@rc"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"fzaninotto/faker": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Core\\": "src/Core"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\": "app/"
		},
		"classmap": [
			"tests/",
			"database/"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
